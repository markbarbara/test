{
  "name": "Hehe Xd",
  "version": "1.1",
  "description": "A browser action with a popup dump of all bookmarks, including search, add, edit and delete.",
  "permissions": [
    "storage",
    "notifications"
  ],
  "browser_action": {
      "default_title": "Hehe XD",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "background": {
     "scripts": [ "background.js" ],
     "persistent": false
  }
}
